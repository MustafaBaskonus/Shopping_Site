@using product.model2.ViewModels;


@model ProductListPartialViewModel



<table class="table table-striped table-hover">
    <thead>
        <tr>
            <td>id</td>
            <td>Ürün</td>
            <td>Fiyat</td>
            <td>Stok</td>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Products)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Price</td>
                <td>@item.Stock</td>
            </tr>
        }

    </tbody>

</table>


